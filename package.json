{
  "name": "rsuite",
  "version": "6.1.1",
  "description": "A suite of react components",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "typings": "esm/index.d.ts",
  "homepage": "https://rsuitejs.com",
  "directories": {
    "lib": "cjs/"
  },
  "scripts": {
    "prepublishOnly": "node ../scripts/validate-builds.js"
  },
  "keywords": [
    "react",
    "rsuite",
    "component",
    "react-component"
  ],
  "author": "React Suite Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:rsuite/rsuite.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "@juggle/resize-observer": "^3.4.0",
    "@rsuite/icons": "^1.4.0",
    "@types/lodash": "^4.17.15",
    "@types/react-window": "^1.8.8",
    "classnames": "^2.3.1",
    "date-fns": "^4.1.0",
    "dom-lib": "^3.3.1",
    "lodash": "^4.17.21",
    "react-textarea-autosize": "^8.5.9",
    "react-use-set": "^1.0.0",
    "react-window": "^1.8.11",
    "rsuite-table": "^5.19.2",
    "schema-typed": "^2.4.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "overrides": {
    "aria-query@>5.1.3": "5.1.3"
  },
  "browserslist": [
    "defaults"
  ],
  "sideEffects": [
    "cjs/**/styles/*",
    "esm/**/styles/*",
    "dist/*",
    "*.scss",
    "*.css"
  ],
  "size-limit": [
    {
      "path": "lib/dist/rsuite.min.js",
      "limit": "300 kB",
      "gzip": true
    }
  ],
  "sass": "styles/index.scss",
  "style": "dist/rsuite.css"
}
